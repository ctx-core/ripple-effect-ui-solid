{
	"name": "@ctx-core/ripple-effect-ui-solid",
	"version": "1.0.78",
	"description": "ctx-core ripple-effect Solidjs component",
	"keywords": [
		"ctx-core",
		"ripple-effect"
	],
	"homepage": "https://github.com/ctx-core/ripple-effect-ui-solid#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/ripple-effect-ui-solid/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/ripple-effect-ui-solid.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"module": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"svelte": "./lib/index.js",
	"scripts": {
		"build": "npm-run-all --parallel build:vite build:types",
		"build:types": "tsc --build",
		"build:vite": "vite build -c vite.config.ts",
		"clean": "rimraf lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"test": "pnpm test-unit",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/ripple-effect": "^13.1.23"
	},
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@ctx-core/object": "^23.1.0",
		"@swc/cli": "^0.1.59",
		"@swc/core": "^1.3.25",
		"babel-preset-solid": "^1.6.7",
		"c8": "^7.12.0",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"solid-js": "^1.6.8",
		"tslib": "^2.4.1",
		"tsx": "^3.12.1",
		"typescript": "next",
		"uvu": "^0.5.6",
		"vite": "^4.0.4",
		"vite-plugin-solid": "^2.5.0"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}