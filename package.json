{
	"name": "@ctx-core/ripple-effect-ui-solid",
	"version": "0.1.25",
	"description": "ctx-core ripple-effect Solidjs component",
	"keywords": [
		"ctx-core",
		"ripple-effect"
	],
	"homepage": "https://github.com/ctx-core/ripple-effect-ui-solid#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/ripple-effect-ui-solid/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/ripple-effect-ui-solid.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"module": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"svelte": "./lib/index.js",
	"scripts": {
		"build": "npm-run-all --parallel build:vite build:types",
		"build:types": "tsc --build",
		"build:vite": "vite build -c vite.config.ts",
		"clean": "rimraf lib",
		"prepublishOnly": "npm run clean && npm run build",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsm node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit",
		"exec": "$@"
	},
	"dependencies": {
		"@ctx-core/ripple-effect": "^13.1.12"
	},
	"devDependencies": {
		"@babel/core": "^7.17.10",
		"@ctx-core/object": "^22.1.10",
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.2.185",
		"babel-preset-solid": "^1.4.0",
		"c8": "^7.11.3",
		"check-dts": "^0.6.7",
		"jsdom": "^19.0.0",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"solid-js": "^1.4.1",
		"solid-styled-components": "^0.28.1",
		"tslib": "^2.4.0",
		"tsm": "^2.2.1",
		"typescript": "next",
		"uvu": "^0.5.3",
		"vite": "^2.9.9",
		"vite-plugin-solid": "^2.2.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}