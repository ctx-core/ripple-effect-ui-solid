{
	"name": "@ctx-core/ripple-effect-ui-solid",
	"version": "0.1.2",
	"description": "ctx-core ripple-effect Solidjs component",
	"keywords": [
		"ctx-core",
		"ripple-effect"
	],
	"homepage": "https://github.com/ctx-core/ripple-effect-ui-solid#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/ripple-effect-ui-solid/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/ripple-effect-ui-solid.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"source": "./src/index.ts",
	"module": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"svelte": "./lib/index.js",
	"scripts": {
		"build": "npm-run-all --parallel build:vite build:types",
		"build:types": "tsc --build",
		"build:vite": "vite build -c vite.config.ts",
		"clean": "rimraf lib",
		"prepublishOnly": "npm run clean && npm run build",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsm node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit",
		"exec": "$@"
	},
	"dependencies": {
		"@ctx-core/ripple-effect": "workspace:^13.1.1"
	},
	"devDependencies": {
		"@babel/core": "^7.16.7",
		"@ctx-core/object": "workspace:^22.0.9",
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.125",
		"babel-preset-solid": "^1.2.6",
		"c8": "^7.11.0",
		"check-dts": "^0.6.6",
		"jsdom": "^19.0.0",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"solid-js": "^1.2.6",
		"solid-styled-components": "^0.27.3",
		"tslib": "^2.3.1",
		"tsm": "^2.2.1",
		"typescript": "next",
		"uvu": "^0.5.2",
		"vite": "^2.7.10",
		"vite-plugin-solid": "^2.1.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
